<h1>Admin Dashboard</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>User</th>
      <th>Allowed to publish?</th>
      <th>All Posts</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.banned ? "No" : "Yes" %></td>
      <td><%= link_to "All public posts", admin_user_path(user.username) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
